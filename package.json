{
  "name": "micro-wishes-github",
  "version": "0.0.0",
  "description": "An easily-deployable feedback microservice that posts GitHub issues on a given repo",
  "files": [
    "index.js"
  ],
  "scripts": {
    "start": "micro index.js",
    "dev": "NODE_ENV=dev nodemon --exec \"npm start\" -e \"js\"",
    "test": "npm run lint && NODE_ENV=test GH_REPO=wishesjs/testrepo GH_TOKEN=secret ava",
    "test:debug": "NODE_ENV=test iron-node ./node_modules/ava/profile.js test.js",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=7"
  },
  "keywords": [
    "micro",
    "wishes",
    "microservice",
    "feedback",
    "github",
    "issues"
  ],
  "repository": "wishesjs/micro-wishes-github",
  "author": "Steven Loria",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "axios-mock-adapter": "^1.8.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "dotenv": "^4.0.0",
    "micro": "^7.3.2",
    "micro-wishes": "^0.4.0",
    "parse-github-repo-url": "^1.4.0"
  }
}
